{
 "name": "lazyjira",
 "description": "opens jira tickets for you with defualt values",
 "version": "1.0",
 "author": "yuvals",

"background": {
      "scripts": ["background.js"],
      "persistent": false
    },
 "page_action": {
      "default_popup": "popup.html",
       "default_icon": {
       	"16": "lazycat.jpg",
		"32": "lazycat.jpg",
		"64": "lazycat.jpg",
	   "128": "lazycat.jpg"
       }
    },
"permissions": ["declarativeContent", "storage"],

 "manifest_version": 2
}
